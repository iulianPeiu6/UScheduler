{
  "Auth0": {
    "Domain": "uscheduler.eu.auth0.com",
    "Audience": "https://uscheduler-api-gateway.azurewebsites.net"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/Workspaces",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "uscheduler-api-workspaces.azurewebsites.net",
          "Port": "443"
        }
      ],
      "RateLimitOptions": {
        "ClientWhitelist": [

        ],
        "EnableRateLimiting": false,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 1
      },
      "UpstreamPathTemplate": "/api/v1/Workspaces",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Workspaces/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "uscheduler-api-workspaces.azurewebsites.net",
          "Port": "443"
        }
      ],
      "RateLimitOptions": {
        "ClientWhitelist": [

        ],
        "EnableRateLimiting": false,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 1
      },
      "UpstreamPathTemplate": "/api/v1/Workspaces/{everything}",
      "UpstreamHttpMethod": [
        "PUT",
        "PATCH",
        "DELETE"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Workspaces?ownerId={everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "uscheduler-api-workspaces.azurewebsites.net",
          "Port": "443"
        }
      ],
      "RateLimitOptions": {
        "ClientWhitelist": [

        ],
        "EnableRateLimiting": false,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 1
      },
      "UpstreamPathTemplate": "/api/v1/Workspaces?ownerId={everything}",
      "UpstreamHttpMethod": [
        "GET"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://uscheduler-api-gateway.azurewebsites.net"
  }
}
